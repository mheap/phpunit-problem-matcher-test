{
  "problemMatcher": [ {
    "owner": "phpunit-failure",
    "fileLocation": ["absolute"],
    "severity": "error",
    "pattern": [{
      "regexp": "^##teamcity\\[testFailed.+message='([^']+)'.+details='\\s+{{GITHUB_WORKSPACE}}([^:]+):(\\d+)[^']+'.+$",
      "message": 1,
      "file": 2,
      "line": 3
    }]
  }]
}
